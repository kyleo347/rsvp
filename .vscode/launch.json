{
        "version": "0.2.0",
        "configurations": [

            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${file}",
                "outFiles": [
                    "${workspaceRoot}/out/**/*.js"
                ]
            },
            {
                "name": "Launch localhost",
                "type": "chrome",
                "request": "launch",
                "url": "http://localhost:4200",
                "webRoot": "${workspaceRoot}/",
                "sourceMaps": true
            },
        ]
    }